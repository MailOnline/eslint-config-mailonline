{
  "extends": [
    "canonical"
  ],
  "rules": {
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "filenames/match-regex": [
      2,
      "^[A-Z]?[a-z]+(?:[A-Z][a-z]+)*(\\.[a-z]+)*$",
      false
    ],
    "no-restricted-syntax": "off",
    "react/forbid-component-props": "off",
    "react/jsx-no-bind": "off",
    "react/sort-prop-types": "off"

  }
}
