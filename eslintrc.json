{
    "extends": "canonical",
    "rules": {
        "arrow-body-style": "off",
        "import/no-commonjs": "off",
        "indent": [
            2,
            2
        ],
        "lodash/prefer-lodash-typecheck": "off",
        "max-len": [2, 120],
        "no-process-exit": "off",
        "no-restricted-syntax": "off",
        "promise/no-native": "off",
        "react/forbid-component-props": "off",
        "react/jsx-indent": [
            2,
            2
        ],
        "react/jsx-indent-props": [
            2,
            2
        ],
        "strict": "off"
    }
}
